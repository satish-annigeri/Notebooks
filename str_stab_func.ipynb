    "%matplotlib inline\n",
    "\n",
    "from numpy import sqrt, sin, cos\n",
    "\n",
    "def stab_func(phi):\n",
    "    x = sqrt(phi)\n",
    "    r = x * (sin(x) - x*cos(x)) / (2*(1-cos(x)) - x*sin(x))\n",
    "    c = (-x + sin(x)) / (x*cos(x) - sin(x))\n",
    "    return r, c\n",
    "\n",
    "r, c = stab_func(10.0)\n",
    "print r, c"
   ]
